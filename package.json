{
  "name": "decks",
  "version": "1.3.7",
  "description": "Decks - Spaced repetition flashcards for Obsidian with FSRS algorithm",
  "main": "dist/main.js",
  "source": "src/main.ts",
  "scripts": {
    "build": "node esbuild.config.mjs production",
    "build:dev": "node esbuild.config.mjs",
    "clean": "rimraf dist",
    "build:release": "npm run clean && npm run build",
    "release-notes": "node scripts/generate-release-notes.mjs",
    "version": "node version-bump.mjs",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "format": "prettier --write \"src/**/*.{ts,js,svelte,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,js,svelte,json}\"",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "test": "jest",
    "test:unit": "jest --testPathIgnorePatterns=integration",
    "test:integration": "jest --config jest.integration.config.js",
    "test:all": "npm run test:unit && npm run test:integration"
  },
  "keywords": [
    "obsidian",
    "flashcards",
    "decks",
    "spaced-repetition",
    "fsrs"
  ],
  "author": "Xherdi Lika",
  "license": "MIT",
  "dependencies": {
    "chart.js": "^4.5.1",
    "chartjs-adapter-date-fns": "^3.0.0",
    "date-fns": "^4.1.0",
    "svelte": "^5.46.1",
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@esbuild-plugins/node-modules-polyfill": "^0.2.2",
    "@eslint-community/eslint-plugin-eslint-comments": "^4.5.0",
    "@eslint/config-array": "^0.23.0",
    "@eslint/object-schema": "^3.0.0",
    "@rollup/plugin-commonjs": "^29.0.0",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-typescript": "^12.3.0",
    "@tsconfig/svelte": "^5.0.6",
    "@types/jest": "^30.0.0",
    "@types/node": "^25.0.3",
    "@types/sql.js": "^1.4.9",
    "@typescript-eslint/eslint-plugin": "^8.50.1",
    "@typescript-eslint/parser": "^8.50.1",
    "builtin-modules": "^5.0.0",
    "esbuild": "^0.27.2",
    "esbuild-plugin-copy": "^2.1.1",
    "esbuild-svelte": "^0.9.4",
    "eslint": "^9.39.2",
    "eslint-plugin-obsidianmd": "^0.1.9",
    "eslint-plugin-svelte": "^3.13.1",
    "glob": "^13.0.0",
    "jest": "^30.2.0",
    "lru-cache": "^11.2.4",
    "obsidian": "^1.11.0",
    "prettier": "^3.7.4",
    "prettier-plugin-svelte": "^3.4.1",
    "rimraf": "^6.1.2",
    "rollup": "^4.54.0",
    "rollup-plugin-svelte": "^7.2.3",
    "sql.js": "^1.13.0",
    "svelte-check": "^4.3.5",
    "svelte-preprocess": "^6.0.3",
    "ts-jest": "^29.4.6",
    "typescript": "^5.9.3"
  }
}